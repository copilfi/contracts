# Smart Contracts Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Avalanche Network RPCs
AVALANCHE_MAINNET_RPC_URL="https://avax-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY"
AVALANCHE_FUJI_RPC_URL="https://avax-fuji.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY"

# Private Key for Deployment (NEVER use a real key with funds!)
PRIVATE_KEY="your_deployment_private_key_here"

# Etherscan API Key for Verification
ETHERSCAN_API_KEY="your_etherscan_api_key"

# Other Chain RPCs (if needed)
ETHEREUM_RPC_URL="your_ethereum_rpc_url"
BASE_RPC_URL="your_base_rpc_url"

# Contract Addresses (will be populated after deployment)
WORKFLOW_MANAGER_ADDRESS=""
CHAINLINK_AUTOMATION_REGISTRY=""
LINK_TOKEN_ADDRESS=""
